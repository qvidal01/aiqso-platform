        "responseBody": "={\"success\": true, \"tenant_id\": \"{{$node[\"Create Tenant Record\"].json[\"id\"]}}\", \"subdomain\": \"{{$node[\"Create Tenant Record\"].json[\"subdomain\"]}}\", \"url\": \"https://{{$node[\"Create Tenant Record\"].json[\"subdomain\"]}}.aiqso.io\"}",
        "options": {}
      },
      "name": "Webhook Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [2010, 300]
    }
