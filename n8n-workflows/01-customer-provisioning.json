    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO tenants (subdomain, company_name, contact_email, stripe_customer_id, status, trial_ends_at)\nVALUES ('{{$json[\"data\"][\"object\"][\"metadata\"][\"subdomain\"]}}', '{{$json[\"data\"][\"object\"][\"metadata\"][\"company_name\"]}}', '{{$json[\"data\"][\"object\"][\"receipt_email\"]}}', '{{$json[\"data\"][\"object\"][\"customer\"]}}', 'active', NOW() + INTERVAL '14 days')\nRETURNING id, subdomain;",
        "options": {}
      },
      "name": "Create Tenant Record",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 1,
      "position": [690, 300],
      "credentials": {
        "postgres": {
          "name": "AIQSO Master DB"
        }
      }
    },
    {
      "parameters": {
        "command": "cd /home/cyber/aiqso-platform/scripts && ./provision-tenant.sh '{{$json[\"id\"]}}' '{{$json[\"subdomain\"]}}' '{{$node[\"Stripe Webhook\"].json[\"data\"][\"object\"][\"metadata\"][\"company_name\"]}}' '{{$node[\"Stripe Webhook\"].json[\"data\"][\"object\"][\"receipt_email\"]}}'",
        "options": {}
      },
      "name": "Execute Provisioning Script",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [910, 300]
    },
    {
      "parameters": {
        "url": "http://10.10.10.127:81/api",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "domain_names",
              "value": "=['{{$node[\"Create Tenant Record\"].json[\"subdomain\"]}}.aiqso.io']"
            },
            {
              "name": "forward_host",
              "value": "={{$node[\"Execute Provisioning Script\"].json[\"stdout\"] | match('IP Address:\\s+(\\d+\\.\\d+\\.\\d+\\.\\d+)')[1]}}"
            },
            {
              "name": "forward_port",
              "value": "80"
            },
            {
              "name": "ssl_forced",
              "value": "true"
            },
            {
              "name": "certificate_id",
              "value": "1"
            }
          ]
        },
        "options": {}
      },
      "name": "Create NginxPM Route",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [1130, 300]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO subscriptions (tenant_id, stripe_subscription_id, status, current_period_start, current_period_end)\nVALUES ('{{$node[\"Create Tenant Record\"].json[\"id\"]}}', '{{$node[\"Stripe Webhook\"].json[\"data\"][\"object\"][\"id\"]}}', 'active', NOW(), NOW() + INTERVAL '1 month');",
        "options": {}
      },
      "name": "Create Subscription Record",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 1,
      "position": [1350, 300]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO audit_log (tenant_id, action, details, ip_address)\nVALUES ('{{$node[\"Create Tenant Record\"].json[\"id\"]}}', 'tenant_provisioned', '{\"subdomain\": \"{{$node[\"Create Tenant Record\"].json[\"subdomain\"]}}\"}', '{{$node[\"Stripe Webhook\"].json[\"request\"][\"headers\"][\"x-forwarded-for\"]}}'::inet);",
        "options": {}
      },
      "name": "Log Audit Entry",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 1,
      "position": [1570, 300]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://10.10.10.104:9000/api/tx",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "subscriber_email",
              "value": "={{$node[\"Stripe Webhook\"].json[\"data\"][\"object\"][\"receipt_email\"]}}"
            },
            {
              "name": "template_id",
              "value": "1"
            },
            {
              "name": "data",
              "value": "={\"subdomain\": \"{{$node[\"Create Tenant Record\"].json[\"subdomain\"]}}\", \"company_name\": \"{{$node[\"Stripe Webhook\"].json[\"data\"][\"object\"][\"metadata\"][\"company_name\"]}}\"}"
            }
          ]
        },
        "options": {}
      },
      "name": "Send Welcome Email",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [1790, 300]
    },
    {
      "parameters": {
